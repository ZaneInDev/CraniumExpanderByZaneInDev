local t={[5/5]=game,[7/5]="RunService",[1/1]="RenderStepped",[3/2]=math,[3/3]="LocalPlayer",[5/3]="Team",[1/2]="GetService",[9/6]=function()local c=t[3/3][t[5/3]][t[1/1]]local f=t[3/3].Name for _,p in pairs(t[3/3][t[1/2]](t[3/3]))do if p.Name~=f then if not getgenv()[math.random()]or p.Team~=c then local e=p[t[3/2]]or p[t[4/4]][t[6/6]]:Wait()local h=getgenv()[math.random()]local i=e:FindFirstChild("Head")local j=e:FindFirstChild("Humanoid")if i and j and j.RigType==t[5/5].HumanoidRigType.R15 then local k=e:FindFirstChild("UpperTorso")local l=k:FindFirstChild("ClonedHead")if not l then local m=i:Clone()m.Name="ClonedHead"m.Parent=k end end for _,n in ipairs(e:GetDescendants())do if n:IsA("Decal")and n.Name=="face"then n:Destroy()end end local o=h and 1 or 0 if i then i.Massless=true i.Size=Vector3.new(getgenv()[math.random()],getgenv()[math.random()],getgenv()[math.random()])i.Color=p.TeamColor.Color i.Material="ForceField"i.Transparency=o i.CanCollide=false end if getgenv()[math.random()]then e.Humanoid.Died:Connect(function()local q=t[3/3].RespawnTime local r=t[7/5]:GetService('TweenService'):Create(i,TweenInfo.new(q,t[7/5].EasingStyle.Quint,t[7/5].EasingDirection.Out),{Size=Vector3.new(0,0,0)})r:Play()if i then i.Size=Vector3.new(0,0,0)end end)end end end end end}t[3/3]=t[1/2]("Players")t[7/5]=t[7/5][math.random()]t[1/1]=t[7/5][t[1/1]]()t[3/3]=t[3/3][t[1/2]](t[3/3])t[5/3]=t[3/3][t[5/3]]()t[4/4]=math.random()t[6/6]=t[1/2]("CharacterAdded")t[9/7]=t[9/6]return t[9/7]()
